	<queryString language="SQL">
		<![CDATA[SELECT p."ProductName",
	p."QuantityPerUnit",
	p."UnitsInStock",
	p."UnitsOnOrder",
	p."UnitPrice",
	s."CompanyName"
FROM "Products" p
	JOIN "Suppliers" s ON 
	 p."SupplierID" = s."SupplierID" 
