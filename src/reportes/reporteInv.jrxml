	<queryString language="SQL">
		<![CDATA[SELECT p."ProductName",
	p."QuantityPerUnit",
	p."UnitsInStock",
	p."UnitsOnOrder",
	p."UnitPrice",
	s."CompanyName"
FROM "Products" p
	JOIN "Suppliers" s ON 
	 p."SupplierID" = s."SupplierID" 
WHERE 
	 p."UnitsInStock" > 1 
	 OR p."UnitsOnOrder" > 1 
ORDER BY p."ProductName" DESC]]>
	</queryString>
